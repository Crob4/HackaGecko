<workspace name="nanoprint">
  <project name="nanoprint" device="EFM32GG990F1024">
    <directories>
      <cmsis>$PROJ_DIR$\..\..\..\..\..</cmsis>
      <bsp>$PROJ_DIR$\..\..\..\..\common\bsp</bsp>
      <kitconfig>$PROJ_DIR$\..\..\..\config</kitconfig>
    </directories>
    <cflags>
      <optimize>none</optimize>
      <define>DEBUG_EFM</define>
    </cflags>
    <includepaths>
      <path>##em-path-cmsis##\CMSIS\Include</path>
      <path>##em-path-cmsis##\Device\EnergyMicro\EFM32GG\Include</path>
      <path>##em-path-cmsis##\emlib\inc</path>
      <path>##em-path-bsp##</path>
      <path>##em-path-kitconfig##</path>
    </includepaths>
    <group name="CMSIS">
      <source>##em-path-cmsis##\Device\EnergyMicro\EFM32GG\Source\$IDE$\startup_efm32gg.s</source>
      <source>##em-path-cmsis##\Device\EnergyMicro\EFM32GG\Source\system_efm32gg.c</source>
    </group>
    <group name="bsp">
      <source>##em-path-bsp##\bsp_bcc.c</source>
      <source>##em-path-bsp##\bsp_stk.c</source>
      <source>##em-path-bsp##\bsp_stk_leds.c</source>
      <source>##em-path-bsp##\bsp_trace.c</source>
    </group>
    <group name="emlib">
      <source>##em-path-cmsis##\emlib\src\em_assert.c</source>
      <source>##em-path-cmsis##\emlib\src\em_cmu.c</source>
      <source>##em-path-cmsis##\emlib\src\em_ebi.c</source>
      <source>##em-path-cmsis##\emlib\src\em_emu.c</source>
      <source>##em-path-cmsis##\emlib\src\em_gpio.c</source>
      <source>##em-path-cmsis##\emlib\src\em_system.c</source>
      <source>##em-path-cmsis##\emlib\src\em_usart.c</source>     
    </group>
    <group name="Source">
      <source>$PROJ_DIR$\..\main.c</source>
      <source>$PROJ_DIR$\..\nprint.c</source>
      <source>$PROJ_DIR$\..\nprint.h</source>            
    </group>
  </project>
</workspace>
